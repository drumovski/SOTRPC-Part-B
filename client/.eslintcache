v>\n        <label>Details</label>\n        <textarea\n          name=\"details\"\n          placeholder=\"Enter details...\"\n          onChange={handleChange}\n        ></textarea>\n      </div>\n      <div>\n        <label>Time</label>\n        <input\n          type=\"text\"\n          name=\"time\"\n          placeholder=\"Enter class timings...\"\n          onChange={handleChange}\n        ></input>\n      </div>\n      <div>\n        <label>Max number</label>\n        <input type=\"number\" name=\"maxNumber\" onChange={handleChange}></input>\n      </div>\n      <div>\n        <label>Teacher</label>\n        <input\n          type=\"text\"\n          name=\"teacher\"\n          placeholder=\"Enter teacher's name...\"\n          onChange={handleChange}\n        ></input>\n      </div>\n      <button onClick={() => history.goBack()}>Back</button>\n      <input type=\"submit\" value=\"Create Class\"></input>\n    </form>\n  );\n};\n\nexport default NewClass;\n","/Users/vipulseth/Desktop/code/flex_track/term_3/SOTRPC-Part-B/client/src/components/Members/Members.js",["72"],"/Users/vipulseth/Desktop/code/flex_track/term_3/SOTRPC-Part-B/client/src/App.js",[],"/Users/vipulseth/Desktop/code/flex_track/term_3/SOTRPC-Part-B/client/src/data/members_data.js",["73"],"/Users/vipulseth/Desktop/code/flex_track/term_3/SOTRPC-Part-B/client/src/components/EditMember/EditMember.js",["74"],"/Users/vipulseth/Desktop/code/flex_track/term_3/SOTRPC-Part-B/client/src/components/Home/Home.js",[],{"ruleId":"75","replacedBy":"76"},{"ruleId":"77","replacedBy":"78"},{"ruleId":"79","severity":1,"message":"80","line":2,"column":1,"nodeType":"81","endLine":10,"endColumn":2},{"ruleId":"82","severity":1,"message":"83","line":3,"column":3,"nodeType":"84","messageId":"85","endLine":9,"endColumn":4},{"ruleId":"75","replacedBy":"86"},{"ruleId":"77","replacedBy":"87"},{"ruleId":"88","severity":1,"message":"89","line":17,"column":6,"nodeType":"90","endLine":17,"endColumn":8,"suggestions":"91"},{"ruleI